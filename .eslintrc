{
  "extends": "@iteam/eslint-config-iteam-node",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "no-useless-concat": "error",
    "prefer-template": "error",
    "quotes": ["error", "single", { "allowTemplateLiterals": false }]
  }
}
